{
  "name": "starlight-theme-galaxy-monorepo",
  "version": "0.1.0",
  "license": "MIT",
  "description": "A customized Astro Starlight theme for publishing docs and educational content.",
  "author": "frostybee",
  "type": "module",
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >=21.0.0"
  },
  "keywords": [
    "starlight",
    "theme",
    "galaxy",
    "plugin",
    "documentation",
    "astro"
  ],
  "packageManager": "pnpm@10.26.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start:docs": "pnpm -F starlight-theme-galaxy-docs dev",
    "build:docs": "pnpm -F starlight-theme-galaxy-docs build",
    "lint": "astro check --noSync && pnpm -r lint",
    "ci-version": "changeset version && pnpm install --no-frozen-lockfile",
    "ci-publish": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^25.0.3",
    "astro": "^5.16.6",
    "eslint": "^9.39.2"
  },
  "homepage": "https://github.com/frostybee/starlight-theme-galaxy",
  "repository": {
    "type": "git",
    "url": "https://github.com/frostybee/starlight-theme-galaxy.git"
  },
  "bugs": "https://github.com/frostybee/starlight-theme-galaxy/issues"
}
